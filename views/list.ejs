<% include parts/header %>

    <div class="container">
        <div class="row">
            <% for(var i = 0; i < list.length; i++) { %>
                <div class="col-md-4">
                    <div class="panel panel-info">
                        <div class="panel-heading">
                            <h4>
                                <%= list[i].title %> <small><%= list[i].date %></small></h4>
                        </div>
                        <div class="panel-body">
                            <%= list[i].note %>
                        </div>
                        <div class="panel-footer">
                            <form action="/deleteNote" method="POST">
                                <button class="btn btn-default" name="id" value="<%= list[i].id %>">deleted</button>
                            </form>
                        </div>
                    </div>
                </div>
                <% }%>
        </div>
    </div>

    <% include parts/footer %>